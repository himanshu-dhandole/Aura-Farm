# Example environment variables for keeper.js

# Ethereum RPC URL
RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Keeper private key (never commit your real key!)
KEEPER_PRIVATE_KEY=your_private_key_here

# Yield Vault contract address
AURA_VAULT_ADDRESS=0xYourVaultAddressHere

# AI API endpoint (optional, default provided in code)
AI_API_URL=https://api.insightyield.ai/allocate-pools

# MongoDB connection URI
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/your-db
